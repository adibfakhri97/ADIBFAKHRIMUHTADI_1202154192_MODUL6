<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adib.adibfakhrimuhtadi_1202154192_modul6">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.adib.adibfakhrimuhtadi_1202154192_modul6.RecyclerViewLayout"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.adib.adibfakhrimuhtadi_1202154192_modul6.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.adib.adibfakhrimuhtadi_1202154192_modul6.TambahGambar"
            android:parentActivityName="com.adib.adibfakhrimuhtadi_1202154192_modul6.tabLayout"></activity>
        <activity
            android:name="com.adib.adibfakhrimuhtadi_1202154192_modul6.tabLayout"
            android:label="Beranda"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.adib.adibfakhrimuhtadi_1202154192_modul6.SignUp" android:parentActivityName="com.adib.adibfakhrimuhtadi_1202154192_modul6.Login"/>
        <activity android:name="com.adib.adibfakhrimuhtadi_1202154192_modul6.DetailGambar" android:parentActivityName="com.adib.adibfakhrimuhtadi_1202154192_modul6.tabLayout"></activity>
    </application>

</manifest>